{
  "buildCommand": "cd levefit && npm install && npm run build && cd ../backend && npm install && npm run build",
  "outputDirectory": "levefit/dist",
  "installCommand": "npm install",
  "framework": "vite",
  "rewrites": [
    {
      "source": "/api/(.*)",
      "destination": "/api/server.js"
    },
    {
      "source": "/(.*)",
      "destination": "/"
    }
  ]
}
